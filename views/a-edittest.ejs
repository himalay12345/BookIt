                                <form action="/admin/updatetest" method="POST" enctype="multipart/form-data">
                                    <input type="hidden" name="id" value="<%=test._id%>">
                                    <div class="row form-row">
                                       
                                        <div class="col-12 col-md-3">
                                            <div class="form-group">
                                                <label> Test Name</label>
                                                <input type="text" placeholder="Enter The Test Name" name="testname" class="form-control" value="<%=test.testname %> ">
                                            </div>
                                        </div>
                                        <div class="col-12 col-md-3">
