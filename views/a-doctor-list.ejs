                                                            <%if(!doctor.encrypt){%>
                                                            <a href="/encrypt-password/?id=<%=doctor._id%>">Encrypt</a>
                                                            <%}else{%>
                                                                <a href="/encrypt-password">Decrypt</a>
                                                                <%}%></td>
                                                                <%}%>
<td>
    <%=doctor.password%>
</td>
