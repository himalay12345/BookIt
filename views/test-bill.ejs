			<!-- /Breadcrumb -->
			
			<!-- Page Content -->
			<div class="content">
				<div class="container-fluid">

					<div class="row">
						<div class="col-lg-8 offset-lg-2">
							<div class="invoice-content">
								<div class="invoice-item">
									<div class="row">
										<div class="col-md-6" style="margin-bottom: 15px;">
											<div class="navbar-brand logo">
												<img src="<%=assetPath('img/aalogo.png')%>" style="height:50px;width:250px" alt="logo">
											</div>
										</div>
										<div class="col-md-6">
											<p class="invoice-details">
												<strong>Order No:</strong> #111111 <br>
                                                <strong>Issued:</strong> <%=locals.user.booked_test_user[index].date%><br>
                                                
                                                
											</p>
										</div>
									</div>
								</div>
								
								<!-- Invoice Item -->
								<div class="invoice-item" style="margin-bottom: 30px;">
									<div class="row">
										<div class="col-md-6">
											<div class="invoice-info">
												<strong class="customer-text">Tests Booked With Lab</strong>
												<p class="invoice-details invoice-details-two text-left" style="float: left !important;">
													<b><%=lab.name%> </b><br>
													<b><%=lab.contacts.address%></b> <br>
													<b><%=lab.contacts.city%> </b><br>
												</p>
											</div>
										</div>
										<div class="col-md-6">
											<div class="invoice-info invoice-info2">
												<strong class="customer-text">Invoice To</strong>
												<p class="invoice-details">
													<%=locals.user.booked_test_user[index].user.name%> <br>
                                                    <%=locals.user.booked_test_user[index].user.phone%><br>
													<%=locals.user.booked_test_user[index].user.age%> Years, <%=locals.user.booked_test_user[index].user.gender%><br>
													<%=locals.user.booked_test_user[index].address.flatno%><br>
                                                    <%=locals.user.booked_test_user[index].address.street%><br>
                                                    <%=locals.user.booked_test_user[index].address.city%>-<%=locals.user.booked_test_user[index].address.pincode%><br>
													
												</p>
											</div>
										</div>
									</div>
								</div>
								
								<div class="invoice-item invoice-table-wrap">
									<div class="row">
										<div class="col-md-12">
											<div class="table-responsive">
												<table class="invoice-table table table-bordered">
													<thead>
														<tr>
															<th>Description</th>
															<th class="text-center">Quantity</th>
															<th class="text-center">GST</th>
															<th class="text-right">Total</th>
														</tr>
													</thead>
													<tbody>
                                                        <%for(let u of locals.user.booked_test_user[index].tests){%>
														<tr>
															<td><%=u.testname%></td>
															<td class="text-center">1</td>
                                                          

                                                            <td class="text-center">Rs 0</td>
                                                            <td class="text-right">Rs <%=u.testprice%></td>
                                                           	
														</tr>
                                                        <%}%>
													
													</tbody>
												</table>
											</div>
										</div>
										<div class="col-md-6 col-xl-4 ml-auto">
											<div class="table-responsive">
												<table class="invoice-table-two table">
													<tbody>
													<tr>
                                                        <th>Subtotal:</th>
                                                        
                                                        <td><span>Rs <%=locals.user.booked_test_user[index].totalprice%></span></td>
                                                       	
													</tr>
													<!-- <tr>
														<th>Discount:</th>
														<td><span>-10%</span></td>
													</tr> -->
													<tr>
                                                        <th>Total Amount:</th>
                                                        
                                                        <td><span>Rs <%=locals.user.booked_test_user[index].totalprice%></span></td>
                                                      
													</tr>
													</tbody>
												</table>
											</div>
										</div>
									</div>
								</div>
								<!-- /Invoice Item -->
								
