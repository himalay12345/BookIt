                                                             month[5] = "Jun";
                                                             month[6] = "Jul";
                                                             month[7] = "Aug";
                                                             month[8] = "Sept";
                                                             month[9] = "Oct";
                                                             month[10] = "Nov";
                                                             month[11] = "Dec";
                                                   
                                                             day = date.getDate();
                                                             
                                                             if(day < 10)
                                                             {
                                                                day = "0"+day;
                                                             }
                                                             
                                                             return    day  + " " +month[date.getMonth()] + " " + date.getFullYear();
                                                             }
                                                               %>
