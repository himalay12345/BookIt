        function changeText(a) {
            if (a.getAttribute('data-flag') == 'false') {
                a.innerText = "Remove";
                a.classList.add('col-black');
                a.setAttribute('data-flag', 'true');
            } else {
                a.innerText = "Add";
                a.classList.remove('col-black');
                a.setAttribute('data-flag', 'false');
            }

        }
    </script>
